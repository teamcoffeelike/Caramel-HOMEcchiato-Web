<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mypage">

	<select id="myprofile"
			parameterType="Integer"
			resultType="ProfileData">
		SELECT	id,
				name,
				motd,
				profileImage
		  FROM	users
		 WHERE	id = #{userId}
	</select>

</mapper>